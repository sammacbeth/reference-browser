{
  "manifest_version": 2,
  "name": "Webextension API Test",
  "version": "0.0.1",
  "background": {
      "scripts": ["background.js"]
  },
  "permissions": [
      "tabs",
      "webRequest",
      "<all_urls>"
  ],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "run_at": "document_start",
      "js": ["content.js"]
    }
  ]
}